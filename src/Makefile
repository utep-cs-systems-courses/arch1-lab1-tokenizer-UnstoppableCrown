# compiler flags. -g for debug, -03 for optimization
CFLAGS = -g -O3

# first target is built by default
all: uimain


#include
tokenizer.o: tokenizer.h
history.o: history.h
uimain.o: uimain.c

# $@=target, $^=prerequisites
uimain: tokenizer.o history.o uimain.o
	cc -o $@ $^ $(CFLAGS) $(OBJS)

$(OBJS): history.h tokenizer.h

#deletes files generated by compilation
clean:
	rm -f *.o uimain *~ *#

run: uimain
	./uimain
